{
  "name": "hyperswitch-sdk-react-native-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "server": "node server.js",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace HyperswitchSdkReactNativeExample.xcworkspace -scheme HyperswitchSdkReactNativeExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@sentry/react-native": "^5.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "18.3.1",
    "react-native": "^0.75.2",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-hyperswitch-kount": "^0.1.0",
    "react-native-hyperswitch-netcetera-3ds": "^0.1.1",
    "react-native-hyperswitch-scancard": "^0.3.1",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-klarna-inapp-sdk": "^2.1.13",
    "react-native-pager-view": "^6.7.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.22.0",
    "react-native-svg": "^15.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
